{
    "manifest_version": 3,
    "name": "Clickbait Detector",
    "version": "1.0",
    "description": "Predicts if a news article is clickbait or not.",
    "permissions": ["activeTab", "scripting", "contextMenus"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
